#!/usr/bin/env bash
wget -cO /tmp/l_amat.zip http://wireless.fcc.gov/uls/data/complete/l_amat.zip
#Unzip just the file we're interested in
unzip /tmp/l_amat.zip HD.dat -d /tmp
cat l_am_headings.csv /tmp/HD.dat > /tmp/HD-headed.dat


